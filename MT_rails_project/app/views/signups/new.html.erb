<h1>Sign Up</h1>
<%= form_for :signup, url: signups_path do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.text_field :password %>

<%= f.label :confirmpassword %>
<%= f.text_field :confirmpassword %>



<%= f.submit "Join" %>
<% end %>